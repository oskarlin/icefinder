<?xml version="1.0" encoding="utf-8"?>
<Map srs="+proj=merc +lon_0=0 +k=1 +x_0=0 +y_0=0 +a=6378137 +b=6378137 +units=m +no_defs">

<Style name="121" filter-mode="first">
  <Rule>
    <RasterSymbolizer opacity="1" scaling="lanczos" />
  </Rule>
</Style>
<Layer name="121"
  srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>121</StyleName>
    <Datasource>
       <Parameter name="file"><![CDATA[1-2-1.tif]]></Parameter>
       <Parameter name="type"><![CDATA[gdal]]></Parameter>
    </Datasource>
  </Layer>

<Style name="121_2" filter-mode="first">
  <Rule>
    <RasterSymbolizer opacity="0.4" comp-op="screen" scaling="lanczos" />
  </Rule>
</Style>
<Layer name="121_2"
  srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>121_2</StyleName>
    <Datasource>
       <Parameter name="file"><![CDATA[1-2-1.tif]]></Parameter>
       <Parameter name="type"><![CDATA[gdal]]></Parameter>
    </Datasource>
  </Layer>
<Style name="367" filter-mode="first">
  <Rule>
    <RasterSymbolizer opacity="0.4" comp-op="difference" scaling="lanczos" />
  </Rule>
</Style>
<Layer name="367"
  srs="+proj=longlat +ellps=WGS84 +datum=WGS84 +no_defs">
    <StyleName>367</StyleName>
    <Datasource>
       <Parameter name="file"><![CDATA[3-6-7.tif]]></Parameter>
       <Parameter name="type"><![CDATA[gdal]]></Parameter>
    </Datasource>
  </Layer>

</Map>
